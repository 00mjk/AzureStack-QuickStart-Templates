{
  "$schema": "http://schema.management.azure.com/schemas/2014-04-01-preview/deploymentParameters.json#",
  "contentVersion": "1.16.1.28",
  "parameters": {
	"azureUser": {
      "value": "<Your Azure user name>"
    },
    "azurePassword": {
      "value": "<Your secured password to Azure>"
    },
	"azureApplicationId": {
      "value": "<Azure Application ID>"
    },
    "tenantId": {
      "value": "<Your secured tenant Id from Azure Subscription>"
    },
    "uniqueDnsNameForPublicIP": {
      "value": "<Unique DNS Name e.g. vmiodns103115>"
    },
    "uniqueStorageAccountName": {
      "value": "<Unique storage name e.g. vmiosa103115>"
    },
    "location": {
      "value": "local"
    },
    "vmAdminUsername": {
      "value": "<Your secured username for VMs>"
    },
    "vmAdminPassword": {
      "value": "<Your secured password for VMs>"
    },
    "vmCount": {
      "value": 2
    },
    "vmOsSku": {
      "value": "2012-R2-Datacenter"
    }
  }
}